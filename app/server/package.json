{
  "name": "fullstackopen-server",
  "version": "1.0.0",
  "description": "Full stack web app server",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require @babel/register './tests/**/*.spec.js'",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "dev-ts": "tsc --w",
    "build-ts": "yarn prebuild && tsc",
    "start-ts": "yarn build-ts && nodemon src/index.ts",
    "serve-ts": "yarn build-ts && ts-node ./dist/index.js",
    "build-js": "babel src -d dist",
    "start-js": "nodemon src/index.js --exec babel-node",
    "serve-js": "yarn build && node dist/index.js"
  },
  "repository": "https://github.com/sujaykundu777/fullstackopen-js",
  "author": "Sujay Kundu",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/node": "^15.6.0",
    "mocha": "^8.4.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
