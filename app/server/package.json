{
  "name": "fullstackopen-server",
  "version": "1.0.0",
  "description": "Full stack open webapp Server",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require @babel/register './tests/**/*.spec.js'",
    "lint": "eslint . --ext .ts src/*",
    "lint:fix": "eslint . --ext .ts src/* --fix",
    "prettify": "prettier --config .prettierrc.json --write src/**/*.ts",
    "dev-ts": "ts-node-dev --inspect --watch -- ./src/index.ts",
    "start:debug": "node --inspect-brk=5858 ./dist/index.js",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build-ts": "yarn prebuild && tsc",
    "start-ts": "yarn build-ts && nodemon src/index.ts",
    "serve-ts": "yarn build-ts && ts-node ./dist/index.js",
    "build-js": "babel src -d dist",
    "start-js": "nodemon src/index.js --exec babel-node",
    "serve-js": "yarn build && node dist/index.js"
  },
  "repository": "https://github.com/sujaykundu777/fullstackopen-js",
  "author": "Sujay Kundu",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.5",
    "nodemon": "^2.0.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsdoc": "^35.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^8.4.0",
    "prettier": "2.3.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
